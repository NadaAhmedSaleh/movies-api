{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2023",
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    },
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmit": true
  }
}
